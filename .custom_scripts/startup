# !bin/sh

setxkbmap -option caps:swapescape
xset r rate 220 45
# Monitor setup, uncomment this line for main computer.
xrandr --output eDP-1 --off --output DP-1 --off --output DP-2 --off --output DP-3 --primary --mode 2560x1440 -r 144 --pos 1920x0 --rotate normal
# Set wallpaper
feh --bg-scale ~/Pictures/Wallpapers/Christmas.jpg
