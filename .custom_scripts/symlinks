# !bin/sh
# You may need to create some directories.
# For instance compton and .custom_scripts.
ln -s -f ~/git/dotfiles-v2/picom.conf ~/.config/picom/picom.conf
ln -s -f ~/git/dotfiles-v2/init.vim ~/.config/nvim/init.vim
ln -s -f ~/git/dotfiles-v2/i3config ~/.config/i3/config
ln -s -f ~/git/dotfiles-v2/.aliases ~/.aliases
ln -s -f ~/git/dotfiles-v2/.zshrc ~/.zshrc
ln -s -f ~/git/dotfiles-v2/.custom_scripts/startup ~/.custom_scripts/startup
ln -s -f ~/git/dotfiles-v2/loveh-theme.zsh-theme ~/.oh-my-zsh/themes/loveh-theme.zsh-theme
ln -s -f ~/git/dotfiles-v2/alacritty.yml ~/.config/alacritty/alacritty.yml
ln -s -f ~/git/dotfiles-v2/lovehtheme1.vim ~/.config/nvim/colors/lovehtheme1.vim

